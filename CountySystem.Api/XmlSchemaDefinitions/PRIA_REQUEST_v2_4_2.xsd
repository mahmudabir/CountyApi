	<xs:include schemaLocation="MISMO_SIGNATURE_Type.xsd"/>
	<xs:include schemaLocation="PRIA_DOCUMENT_v2_4_1.XSD"/>
	<xs:element name="REQUEST_GROUP" type="PRIA_REQUEST_GROUP_Type"/>
	<xs:complexType name="PRIA_KEY_Type">
		<xs:sequence/>
		<xs:attribute name="_Name" type="xs:string">
			<xs:annotation>
				<xs:documentation>A generic placeholder to add key descriptions in transactions.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Request, Response</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Credit Reporting</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Key Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_Value" type="xs:string">
			<xs:annotation>
				<xs:documentation>A generic placeholder for a value to be associated with a key description for a transaction exchange.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Request, Response</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Credit Reporting</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Key Value</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PRIA_PACKAGE_Type">
		<xs:sequence>
			<xs:element name="PRIA_DOCUMENT" type="PRIA_DOCUMENT_Type" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PAYMENT" type="PRIA_PAYMENT_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="_Identifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>A unique system-assigned identifier (such as an audit trail number) to each package.</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Package Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_SequenceIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>When more than one package is included this indicates the order in which they should be processed</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Package Sequence Indentifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PRIA_PAYMENT_Type">
		<xs:sequence>
			<xs:element name="SERVICE_PAYMENT" type="PRIA_SERVICE_PAYMENT_Type" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="_MethodType" type="PRIA_PaymentMethodType_Enumerated">
			<xs:annotation>
				<xs:documentation>A description of the method of funds transfer to be used for the transaction</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Enumerated</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Method Type</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_MethodTypeOtherDescription" type="xs:string">
			<xs:annotation>
				<xs:documentation>A description of the type of payment when Other is selected from the enumerated list</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Method Type Other Description</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_Amount" type="PRIA_MISMOMoney">
			<xs:annotation>
				<xs:documentation>The total amount of payment being tendered</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Money</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Amount</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_RecorderOnAccountIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>An alphanumeric designation assigned to the prepaid account to be debited for the payment</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Recorder On Account Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_RecorderOnAccountMaximumDebitAmount" type="PRIA_MISMOMoney">
			<xs:annotation>
				<xs:documentation>The maximum dollar amount that can be debited from the prepaid account on any single transaction</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Money</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Recorder On Account Maximum Debit Amount</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_RecorderOnAccountMinimumBalanceAmount" type="PRIA_MISMOMoney">
			<xs:annotation>
				<xs:documentation>The dollar amount at which the system should send a message that the prepaid account needs to be replenished.  It may also trigger the system to discontinue processing documents until more funds are added to the account.</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Money</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Recorder On Account Minimum Balance Amount</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_AccountIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>An alphanumeric designation assigned to the account that payment will be processed from</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Account Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_ReferenceIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>A unique identifier assigned to each payment transaction for tracking and auditing purposes</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Reference Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_SequenceIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>When more than one payment is tendered, this indicates the order in which they should be processed</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Payment Sequence Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PRIA_REQUEST_Type">
		<xs:sequence>
			<xs:element name="PACKAGE" type="PRIA_PACKAGE_Type" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="RECORDING_TRANSACTION_IDENTIFIER" type="PRIA_RECORDING_TRANSACTION_IDENTIFIER_Type" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="_RelatedDocumentsIndicator" type="PRIA_MISMOIndicatorType">
			<xs:annotation>
				<xs:documentation>Indicates whether the documents sent are related to each other.  If "yes", all documents must pass recordability tests for any to be recorded.  If "no", one doucment can fail recordability without preventing the remaining documents from being recorded.</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Boolean</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>PRIA Request Related Documents Indicator</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_Type" type="PRIA_PRIARequestType_Enumerated">
			<xs:annotation>
				<xs:documentation>A description of the action being requested</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Enumerated</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>PRIA Request Type</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_TypeOtherDescription" type="xs:string">
			<xs:annotation>
				<xs:documentation>A description of the type of request when Other is selcted from the enumerated list</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>PRIA Request Type Other Description</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_SequenceIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>When more than one request is included this indicates the order in which they should be processed</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>PRIA Request Sequence Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PRIA_RECEIVING_PARTY_Type">
		<xs:sequence>
			<xs:element name="CONTACT_DETAIL" type="PRIA_CONTACT_DETAIL_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="_Name" type="xs:string">
			<xs:annotation>
				<xs:documentation>Identifies the name of the intended recipient of the XML message.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_StreetAddress" type="xs:string">
			<xs:annotation>
				<xs:documentation>The first line of the receiving partys street address (e.g., 123 Main Street).</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party Street Address</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_StreetAddress2" type="xs:string">
			<xs:annotation>
				<xs:documentation>The second line of the receiving partys street address (e.g., Suite 250).</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party Street Address 2</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_City" type="xs:string">
			<xs:annotation>
				<xs:documentation>The city in which the receiving party is located.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party City</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_State" type="xs:string">
			<xs:annotation>
				<xs:documentation>The state in which the receiving party is located. This is usually entered using the USPS two letter state abbreviation.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party State</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_PostalCode" type="xs:string">
			<xs:annotation>
				<xs:documentation>The 5 or 9 digit postal code in which the receiving party is located.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party Postal Code</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_Identifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>An identifier that is assigned to the receiving party as defined by the trading partners involved in the transaction.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Receiving Party</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Credit Reporting</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Receiving Party Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PRIA_RECORDING_TRANSACTION_IDENTIFIER_Type">
		<xs:sequence/>
		<xs:attribute name="_Type" type="PRIA_RecordingTransactionIdentifierType_Enumerated">
			<xs:annotation>
				<xs:documentation>This enumerated attribute identifies what agency the transaction is intended for</xs:documentation>
				<xs:appinfo>
					<mismo:HISTORY>
						<mismo:ADD version="2.4">PRIA</mismo:ADD>
					</mismo:HISTORY>
					<mismo:DATATYPE>Enumerated</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Recording Transaction Identifier Type</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_TypeOtherDescription" type="xs:string">
			<xs:annotation>
